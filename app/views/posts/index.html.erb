<% @posts.each do |post| %>
  <%= image_tag post.restaurant	.photo %>
  <%= post.restaurant.name %>
  <%= image_tag post.photo %>
  <%= post.description %>
  <% if post.restaurant == current_restaurant %>
    <%= link_to 'Edit Post', edit_post_path(post.id) %>
    <%= link_to 'Delete Post', post_path(post.id), method: :delete %>	
  <% end %>
<% end %>